<?xml version="1.0" encoding="utf-8"?>
<configs>
	<!-- base bicycle is 5,10 -->
    <set xpath="vehicles/vehicle[@name='vehicleBicycle']/property[@name='velocityMax']/@value">10, 15</set>
	<!-- base minibike is 7,10 -->
    <set xpath="vehicles/vehicle[@name='vehicleMinibike']/property[@name='velocityMax']/@value">13, 18</set>
	<!-- base motorycle is 9,14 -->
    <set xpath="vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax']/@value">20, 40</set>
	<!-- base 4x4 is 9,14 -->
    <set xpath="vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax']/@value">18, 30</set>
	<!-- base gyro is 9,15 -->
    <set xpath="vehicles/vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax']/@value">22, 42</set>
</configs>